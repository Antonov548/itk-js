{
  "name": "itk",
  "version": "9.1.2",
  "description": "Read and write 2D, 3D, and ND images with the Insight Toolkit (ITK)",
  "main": "./dist/itk.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "doc": "kw-doc -c ./doc/config.js",
    "doc:www": "kw-doc -c ./doc/config.js -s",
    "doc:publish": "kw-doc -c ./doc/config.js -mp",
    "commit": "git cz",
    "build": "node ./build.js",
    "test": "./build/dockcross bash -c \"cd build && ctest -V\" && standard \"src/*.js\" \"test/**/*.js\" && ava \"test/*.js\" && npm run browser-test",
    "browser-test": "karma start ./karma.conf.js",
    "browser-test:debug": "karma start ./karma.conf.js --no-single-run"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InsightSoftwareConsortium/itk-js.git"
  },
  "keywords": [
    "itk",
    "imaging",
    "io",
    "medical",
    "scientific"
  ],
  "author": "Insight Software Consortium",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/InsightSoftwareConsortium/itk-js/issues"
  },
  "homepage": "https://insightsoftwareconsortium.github.io/itk-js/",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "async": "^2.6.1",
    "ava": "^1.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "commitizen": "3.0.5",
    "cz-conventional-changelog": "2.1.0",
    "file-api": "^0.10.4",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "json-loader": "^0.5.7",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-tap": "^4.1.4",
    "karma-tap-pretty-reporter": "^4.1.0",
    "karma-webpack": "3.0.5",
    "kw-doc": "1.2.2",
    "ramda": "^0.26.1",
    "standard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "uglifyify": "^5.0.1",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "commander": "^2.19.0",
    "mime-types": "^2.1.21",
    "promise-file-reader": "^1.0.2",
    "webworker-promise": "^0.4.2"
  },
  "bin": {
    "itk-js": "./itk-js-cli.js"
  },
  "standard": {
    "ignore": [
      "src/loadEmscriptenModuleBrowser.js",
      "src/itk-js-cli.js",
      "itkJSImageIOPost.js",
      "test/Browser/tests.js"
    ]
  }
}
